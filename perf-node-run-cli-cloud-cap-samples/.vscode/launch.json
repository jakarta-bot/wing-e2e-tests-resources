  "version": "0.2.0",
  "configurations": [
    {
      "name": "bookshop",
      "command": "cds watch bookshop",
      "request": "launch",
      "type": "node-terminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Fiori app",
      "command": "cds watch fiori",
      "request": "launch",
      "type": "node-terminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "sample",
      "description": "Which sample do you want to start?",
      "options": ["bookshop", "fiori", "reviews", "reviews/test/bookshop"],
      "default": "bookshop"
    }
  ]
}
